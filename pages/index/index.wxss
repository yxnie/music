.wrap {
  margin: 0 20rpx;
  padding-bottom: 60rpx;
}
.title {
  display: flex;
  justify-content: space-between;
  margin: 30rpx 0;
  font-size: 36rpx;
}
.left {
  padding-top: 8px;
  color: black;
}
.right {
  padding: 10rpx 18rpx;
  background: #e2e2e2;
  border-radius: 54rpx;
}

.top {
  display: flex;
  justify-content: space-between;
  margin: 10rpx 26rpx;
}
.before {
  font-size: 40rpx;
  position: relative;
  top: 20rpx;
}
.top image {
  display: block;
  width: 68rpx;
  height: 68rpx;
  position: relative;
  top: 20rpx;
}